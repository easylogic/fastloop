{
  "name": "fastloop",
  "version": "0.0.1",
  "description": "Super Fast Loop",
  "main": "./dist/fastloop.js",
  "scripts": {
    "pub": "npm run build && npm publish",
    "build": "NODE_ENV=production rollup --config config/rollup.config.prod.js",
    "dev": "NODE_ENV=development rollup -w --config config/rollup.config.dev.js",
    "docs": "http-server ./docs",
    "test": "NODE_ENV=test jest --watch",
    "benchmark": "node ./benchmark/benchmark.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/easylogic/fastloop.git"
  },
  "keywords": [
    "super",
    "fast",
    "loop",
    "for"
  ],
  "author": "easylogic",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/easylogic/fastloop/issues"
  },
  "homepage": "https://github.com/easylogic/fastloop#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-jest": "^22.4.1",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2015-rollup": "^3.0.0",
    "babel-preset-minify": "^0.2.0",
    "benchmark": "^2.1.4",
    "cross-env": "^5.1.1",
    "http-server": "^0.11.1",
    "jest": "^22.4.2",
    "jest-cli": "^22.4.2",
    "jest-report": "^0.1.11",
    "rollup": "^0.52.1",
    "rollup-plugin-babel": "^3.0.3",
    "rollup-plugin-livereload": "^0.6.0",
    "rollup-plugin-minify": "^1.0.3",
    "rollup-plugin-serve": "^0.4.2"
  }
}
